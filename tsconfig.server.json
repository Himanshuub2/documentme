{
  "compilerOptions": {
      "module": "ES6",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "target": "es6",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "outDir": "./server/dist",
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*",
              "types/*"
          ]
      }
  },
  "include": [
      "server/**/*",
      
  ]
}