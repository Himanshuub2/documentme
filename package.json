{
  "name": "documentme",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "client": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "node-build": "rm -rf ./server/dist  && tsc -p tsconfig.server.json &&  cd ./server/dist  && node index.js",
    "server": "tsx watch ./server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prisma-migrate": "npx prisma migrate dev --schema ./server/prisma/schema.prisma",
    "prisma-generate": "npx prisma generate --schema ./server/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth2": "^0.1.8",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "docx": "^8.5.0",
    "dotenv": "^16.4.5",
    "eslint-config-next": "^14.2.4",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.383.0",
    "next": "^14.2.4",
    "openai": "^4.40.2",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "pdfkit": "^0.15.0",
    "prisma": "^5.13.0",
    "puppeteer": "^22.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pdfkit": "^0.13.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5"
  }
}
